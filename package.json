    "@next/bundle-analyzer": "12.0.3",
    "@types/body-scroll-lock": "3.1.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.176",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.7",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.34",
    "@types/uuid": "8.3.1",
    "concurrently": "6.3.0",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.3.1",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.6.203",
    "next-unused": "0.0.6",
    "pinst": "2.1.6",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "next-unused": {
    "alias": {
      "~components/*": [
        "components/*"
      ],
      "~config/*": [
        "config/*"
      ],
      "~context/*": [
        "context/*"
      ],
      "~lib/*": [
        "lib/*"
      ],
      "~styles/*": [
        "styles/*"
      ],
      "~utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "config",
      "context",
      "hooks",
      "lib",
      "pages",
      "styles",
      "utils"
    ],
    "exclude": [
      "components/Modal/*",
      "components/NowPlaying/NowPlaying.module.css",
      "components/NowPlaying/NowPlayingWithControls.tsx",
      "config/notion/schema/defaults.ts",
      "config/notion/website/*",
      "data/mock/**",
      "lib/firebase.ts",
      "lib/notion/init/*",
      "utils/isObjectEmpty.ts",
      "zzz/*"
    ],
    "entrypoints": [
      "pages"
    ]
  }
}
