{
  "name": "@jeromefitz/jeromefitzgerald.com",
  "private": true,
  "version": "0.0.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "build": "next build",
    "branch": "git-cz -m branch --allow-empty",
    "clean": "./scripts/clean.sh",
    "clean:install": "yarn run clean && yarn install",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "find:unused": "next-unused",
    "lint:eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write && yarn typescript",
    "lint:prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "lint:typescript": "yarn typescript",
    "lint": "yarn typescript && yarn lint:prettier --check && yarn lint:eslint && yarn next lint",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "postpublish": "pinst --enable",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretty-quick": "pretty-quick",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "jest",
    "typescript": "tsc --noEmit --declaration"
  },
  "dependencies": {
    "@heroicons/react": "1.0.4",
    "@notionhq/client": "0.4.0",
    "@plaiceholder/next": "2.1.0",
    "@plaiceholder/tailwindcss": "2.1.0",
    "@reach/alert": "0.16.0",
    "@reach/auto-id": "0.16.0",
    "@reach/portal": "0.16.0",
    "@reach/skip-nav": "0.16.0",
    "@reach/tooltip": "0.16.0",
    "@reach/visually-hidden": "0.16.0",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@vercel/fetch": "6.1.1",
    "ajv": "8.6.3",
    "autoprefixer": "10.3.6",
    "body-scroll-lock": "3.1.5",
    "clsx": "1.1.1",
    "comma-number": "2.1.0",
    "date-fns": "2.24.0",
    "date-fns-tz": "1.1.6",
    "fathom-client": "3.2.0",
    "firebase": "9.1.0",
    "firebase-admin": "9.12.0",
    "framer-motion": "4.1.17",
    "github-slugger": "1.4.0",
    "htmlescape": "1.1.1",
    "inspx": "0.0.1-alpha.6",
    "intersection-observer": "0.12.0",
    "ioredis": "4.27.9",
    "keen-slider": "5.5.1",
    "lodash": "4.17.21",
    "ms": "3.0.0-canary.1",
    "next": "11.1.3-canary.32",
    "next-seo": "4.28.1",
    "next-themes": "0.0.15",
    "node-emoji": "1.11.0",
    "nprogress": "0.2.0",
    "plaiceholder": "2.0.1",
    "pluralize": "8.0.0",
    "podcast": "1.3.0",
    "preact": "10.5.14",
    "react": "17.0.2",
    "react-blurhash": "0.1.3",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-intersection-observer": "8.32.1",
    "react-jsx-parser": "1.28.4",
    "react-merge-refs": "1.1.0",
    "react-resize-aware": "3.1.1",
    "react-use": "17.3.1",
    "sharp": "0.29.1",
    "swr": "1.0.1",
    "tabbable": "5.2.1",
    "tailwind-underline-utils": "1.1.3",
    "tailwindcss": "2.2.16",
    "tinycolor2": "1.4.2",
    "title": "3.4.3",
    "use-sound": "4.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jeromefitz/codestyle": "4.0.4",
    "@jeromefitz/git-cz": "10.0.4",
    "@jeromefitz/semantic": "4.0.4",
    "@next/bundle-analyzer": "11.1.2",
    "@types/body-scroll-lock": "3.1.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/ms": "0.7.31",
    "@types/node": "16.10.1",
    "@types/nprogress": "0.2.0",
    "@types/reach__skip-nav": "0.1.3",
    "@types/react": "17.0.24",
    "@types/uuid": "8.3.1",
    "concurrently": "6.2.2",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint-config-next": "11.1.2",
    "fs-extra": "10.0.0",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest": "27.2.3",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.6.168",
    "next-unused": "0.0.6",
    "pinst": "2.1.6",
    "postcss": "8.3.8",
    "postcss-custom-properties": "12.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-font-variant": "5.0.0",
    "postcss-gap-properties": "3.0.0",
    "postcss-hover-media-feature": "1.0.1",
    "postcss-initial": "4.0.1",
    "postcss-media-minmax": "5.0.0",
    "postcss-nested": "5.0.6",
    "postcss-page-break": "3.0.4",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "next-unused": {
    "alias": {
      "~components/*": [
        "components/*"
      ],
      "~config/*": [
        "config/*"
      ],
      "~context/*": [
        "context/*"
      ],
      "~lib/*": [
        "lib/*"
      ],
      "~styles/*": [
        "styles/*"
      ],
      "~utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "config",
      "context",
      "hooks",
      "lib",
      "pages",
      "styles",
      "utils"
    ],
    "exclude": [
      "components/Modal/*",
      "components/NowPlaying/NowPlaying.module.css",
      "components/NowPlaying/NowPlayingWithControls.tsx",
      "config/notion/schema/defaults.ts",
      "config/notion/website/*",
      "lib/firebase.ts",
      "lib/notion/init/*",
      "lib/spotify/mock/mockTrack.ts",
      "utils/isObjectEmpty.ts",
      "zzz/*"
    ],
    "entrypoints": [
      "pages"
    ]
  }
}
