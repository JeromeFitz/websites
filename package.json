{
  "name": "@jeromefitz/jeromefitzgerald.com",
  "private": true,
  "version": "0.0.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "----": "",
    "clean": "./scripts/clean.sh",
    "clean:install": "yarn run clean && yarn install",
    "-----": "",
    "lint:eslint": "eslint . --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write && yarn typescript",
    "lint:prettier": "prettier \"**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "lint:typescript": "yarn typescript",
    "lint": "yarn typescript && yarn lint:prettier --check && yarn lint:eslint && yarn next lint",
    "pretty-quick": "pretty-quick",
    "typescript": "tsc --noEmit --declaration",
    "------": "",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "postpublish": "pinst --enable",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "semantic-release": "semantic-release",
    "-------": "",
    "branch": "git-cz -m branch --allow-empty",
    "find:unused": "next-unused",
    "--------": "",
    "test": "jest"
  },
  "dependencies": {
    "@bigheads/core": "0.3.3",
    "@heroicons/react": "1.0.5",
    "@notionhq/client": "0.4.10",
    "@plaiceholder/next": "2.2.0",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-accessible-icon": "0.1.3",
    "@radix-ui/react-accordion": "0.1.5",
    "@radix-ui/react-alert-dialog": "0.1.5",
    "@radix-ui/react-announce": "0.1.3",
    "@radix-ui/react-aspect-ratio": "0.1.2",
    "@radix-ui/react-avatar": "0.1.3",
    "@radix-ui/react-checkbox": "0.1.4",
    "@radix-ui/react-compose-refs": "0.1.0",
    "@radix-ui/react-dialog": "0.1.5",
    "@radix-ui/react-dropdown-menu": "0.1.4",
    "@radix-ui/react-icons": "1.0.3",
    "@radix-ui/react-popover": "0.1.4",
    "@radix-ui/react-radio-group": "0.1.4",
    "@radix-ui/react-scroll-area": "0.1.3",
    "@radix-ui/react-separator": "0.1.3",
    "@radix-ui/react-switch": "0.1.4",
    "@radix-ui/react-tabs": "0.1.4",
    "@radix-ui/react-tooltip": "0.1.6",
    "@radix-ui/react-utils": "0.0.5",
    "@radix-ui/react-visually-hidden": "0.1.3",
    "@stitches/react": "1.2.6",
    "@vercel/fetch": "6.1.1",
    "clsx": "1.1.1",
    "csv-parser": "3.0.0",
    "date-fns": "2.27.0",
    "date-fns-tz": "1.1.6",
    "emoji-api": "2.0.1",
    "emoji-regex": "10.0.0",
    "fathom-client": "3.2.0",
    "firebase": "9.6.1",
    "firebase-admin": "10.0.0",
    "framer-motion": "5.5.3",
    "github-slugger": "1.4.0",
    "lodash": "4.17.21",
    "mousetrap": "1.6.5",
    "ms": "3.0.0-canary.1",
    "next": "12.0.8-canary.6",
    "next-seo": "4.28.1",
    "next-themes": "0.0.15",
    "node-emoji": "1.11.0",
    "nprogress": "0.2.0",
    "plaiceholder": "2.2.0",
    "pluralize": "8.0.0",
    "podcast": "2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.1.1",
    "react-use": "17.3.1",
    "sharp": "0.29.3",
    "smoothscroll-polyfill": "0.4.4",
    "swr": "1.1.1",
    "title": "3.4.3",
    "use-sound": "4.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jeromefitz/codestyle": "6.0.6",
    "@jeromefitz/git-cz": "12.0.6",
    "@jeromefitz/semantic": "6.0.5",
    "@next/bundle-analyzer": "12.0.7",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.178",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.13",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.37",
    "@types/uuid": "8.3.3",
    "dotenv": "10.0.0",
    "eslint-config-next": "12.0.7",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.5",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.6.203",
    "next-unused": "0.0.6",
    "pinst": "2.1.6",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "next-unused": {
    "alias": {
      "@jeromefitz/design-system": "src/styles/system/",
      "~components": "src/components/",
      "~config": "src/config/",
      "~context": "src/context/",
      "~hooks": "src/hooks/",
      "~lib": "src/lib/",
      "~styles": "src/styles/",
      "~utils": "src/utils/"
    },
    "debug": false,
    "entrypoints": [
      "src/pages"
    ],
    "exclude": [
      "src/config/websites/*",
      "src/data/*",
      "src/hooks/useProgress.ts",
      "src/hooks/useRefScrollProgress.ts",
      "src/hooks/useScrollCounter.ts",
      "src/hooks/useSettings.ts",
      "src/hooks/useSticky.ts",
      "src/hooks/useOnlineStatus.ts",
      "src/hooks/usePrefersReducedMotion.ts",
      "src/lib/firebase.ts",
      "src/lib/pubsub.ts",
      "src/lib/notion/api/getBlocks.ts",
      "src/lib/notion/api/getDatabasesById.ts",
      "src/lib/notion/api/getPages.ts",
      "src/lib/notion/api/getSearch.ts",
      "src/lib/notion/api/getTypes/*",
      "src/styles/fonts/*",
      "src/utils/isElementOfType.ts",
      "src/utils/isObjectEmpty.ts",
      "src/utils/stringToUUID.ts"
    ],
    "include": [
      "src/components",
      "src/config",
      "src/context",
      "src/hooks",
      "src/lib",
      "src/pages",
      "src/styles",
      "src/utils"
    ]
  }
}
