{
  "name": "website",
  "private": true,
  "version": "1.7.3",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "build": "next build",
    "clean": "./scripts/clean.sh",
    "clean:install": "yarn run clean && yarn install",
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "find:unused": "next-unused",
    "lint-typescript": "yarn typescript",
    "lint-eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0",
    "lint": "yarn lint-typescript && yarn prettier-check && yarn lint-eslint",
    "lint-fix": "yarn prettier-fix && eslint . --ext js,jsx,ts,tsx --fix --max-warnings=0",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "postpublish": "pinst --enable",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "prettier-check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "prettier-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "pretty-quick": "pretty-quick",
    "semantic-release": "semantic-release",
    "start": "next start",
    "typescript": "tsc --noEmit --declaration -p tsconfig.json"
  },
  "dependencies": {
    "@reach/portal": "0.15.0",
    "@reach/skip-nav": "0.15.0",
    "@tailwindcss/typography": "0.4.0",
    "@tailwindcss/ui": "0.7.2",
    "@types/body-scroll-lock": "2.6.1",
    "@vercel/fetch": "6.1.0",
    "autoprefixer": "10.2.5",
    "body-scroll-lock": "3.1.5",
    "clsx": "1.1.1",
    "comma-number": "2.1.0",
    "date-fns": "2.21.3",
    "date-fns-tz": "1.1.4",
    "fathom-client": "3.0.0",
    "firebase": "8.6.1",
    "firebase-admin": "9.8.0",
    "github-slugger": "1.3.0",
    "htmlescape": "1.1.1",
    "intersection-observer": "0.12.0",
    "ioredis": "4.27.2",
    "js-cookie": "2.2.1",
    "keen-slider": "5.4.1",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "next": "10.2.1-canary.2",
    "next-seo": "4.24.0",
    "next-themes": "0.0.14",
    "nprogress": "0.2.0",
    "podcast": "1.3.0",
    "preact": "10.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-intersection-observer": "8.32.0",
    "react-jsx-parser": "1.28.4",
    "react-merge-refs": "1.1.0",
    "swr": "0.5.6",
    "tabbable": "5.2.0",
    "tailwind-underline-utils": "1.1.3",
    "tailwindcss": "2.1.2",
    "title": "3.4.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jeromefitz/codestyle": "1.1.2",
    "@jeromefitz/semantic": "1.1.2",
    "@next/bundle-analyzer": "10.2.2",
    "@types/js-cookie": "2.2.6",
    "@types/ms": "0.7.31",
    "@types/nanoid": "2.1.0",
    "@types/node": "14.14.41",
    "@types/node-fetch": "2.5.10",
    "@types/nprogress": "0.2.0",
    "@types/reach__skip-nav": "0.1.1",
    "@types/react": "17.0.6",
    "@types/uuid": "8.3.0",
    "concurrently": "6.1.0",
    "cross-env": "7.0.3",
    "dotenv": "8.5.1",
    "fs-extra": "9.1.0",
    "globby": "11.0.3",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "1.6.57",
    "next-unused": "0.0.6",
    "pinst": "2.1.6",
    "postcss": "8.3.0",
    "postcss-custom-properties": "11.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-font-variant": "5.0.0",
    "postcss-gap-properties": "git://github.com/armano2/postcss-gap-properties.git#postcss8",
    "postcss-hover-media-feature": "1.0.1",
    "postcss-initial": "4.0.1",
    "postcss-media-minmax": "5.0.0",
    "postcss-nested": "5.0.5",
    "postcss-page-break": "3.0.4",
    "rimraf": "3.0.2",
    "typescript": "4.1.5"
  },
  "next-unused": {
    "alias": {
      "~components/*": [
        "components/*"
      ],
      "~config/*": [
        "config/*"
      ],
      "~context/*": [
        "context/*"
      ],
      "~lib/*": [
        "lib/*"
      ],
      "~styles/*": [
        "styles/*"
      ],
      "~utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "config",
      "context",
      "hooks",
      "lib",
      "pages",
      "styles",
      "utils"
    ],
    "exclude": [
      "components/Modal/*",
      "components/NowPlaying/NowPlaying.module.css",
      "components/NowPlaying/NowPlayingWithControls.tsx",
      "config/notion/schema/defaults.ts",
      "config/notion/website/*",
      "lib/firebase.ts",
      "lib/notion/init/*",
      "lib/spotify/mock/mockTrack.ts",
      "utils/isObjectEmpty.ts"
    ],
    "entrypoints": [
      "pages"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "resolutions": {
    "webpack": "5.35.0"
  }
}
