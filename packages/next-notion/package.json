  "repository": "jeromefitz/websites.git",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup-node",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "pnpm clean && rm -rf node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "dev": "tsup-node --watch",
    "format:lint": "pnpm lint:eslint --fix && pnpm format:prettier --write",
    "format:prettier": "prettier \"./src/**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\" --ignore-unknown --loglevel warn",
    "lint": "pnpm format:prettier --check && pnpm lint:eslint && pnpm lint:typescript",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:typescript": "tsc --noEmit --declaration",
    "semantic-release": "echo semantic-release",
    "test": "echo # jest"
  },
  "dependencies": {
    "@jeromefitz/utils": "2.2.1",
    "fast-json-stable-stringify": "2.1.0",
    "github-slugger": "2.0.0",
    "ms": "3.0.0-canary.1",
    "podcast": "2.0.1",
    "uuid": "9.0.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "@jeromefitz/design-system": "4.1.5",
    "@jeromefitz/notion": "4.0.0",
    "@types/ms": "0.7.31",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "8.3.4",
    "@types/valid-url": "1.0.3",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "ioredis": "5.2.3",
    "next": "13.0.0",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "@jeromefitz/design-system": "^4.1.1",
    "@jeromefitz/notion": "^4.0.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.6",
    "ioredis": "^5.2.1",
    "next": "^13.0.0",
    "plaiceholder": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
