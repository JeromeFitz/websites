    "./blocks/RichText": {
      "types": "./dist/blocks/RichText.d.ts",
      "default": "./dist/blocks/RichText.js"
    },
    "./helper": {
      "types": "./dist/helper.d.ts",
      "default": "./dist/helper.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
