{
  "name": "next-notion",
  "version": "0.0.0",
  "type": "module",
  "repository": "jeromefitz/websites.git",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup-node",
    "clean": "rm -rf ./.turbo ./dist",
    "clean:install": "pnpm clean && rm -rf ./node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "dev": "tsup-node --watch",
    "format:lint": "pnpm lint:eslint --fix",
    "lint": "pnpm lint:eslint && pnpm lint:typescript",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:typescript": "tsc --noEmit --declaration",
    "semantic-release": "echo semantic-release"
  },
  "peerDependencies": {
    "@jeromefitz/utils": "^2.2.2",
    "@notionhq/client": "^2.2.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "emoji-regex": "10.2.1",
    "node-emoji": "2.1.0"
  },
  "devDependencies": {
    "@jeromefitz/ds": "workspace:*",
    "@jeromefitz/utils": "2.2.2",
    "@notionhq/client": "2.2.12",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
