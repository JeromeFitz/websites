{
  "name": "next-notion",
  "version": "0.0.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/websites.git",
    "directory": "packages/next-notion"
  },
  "scripts": {
    "---": "@tsup",
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "--- ": "",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "---  ": "@lint",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:prettier": "prettier \"./src/**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\" --ignore-unknown --loglevel warn",
    "lint:typescript": "tsc --noEmit --declaration",
    "lint": "yarn lint:prettier --check && yarn lint:eslint && yarn lint:typescript",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "---   ": "@release",
    "semantic-release": "echo semantic-release",
    "semantic-release:mono": "echo semantic-release -e semantic-release-monorepo",
    "---    ": "",
    "---     ": "",
    "test": "echo # jest",
    "---      ": ""
  },
  "dependencies": {
    "@jeromefitz/design-system": "3.1.0-next-notion.3",
    "@jeromefitz/notion": "3.1.0-next-notion.3",
    "@jeromefitz/utils": "2.0.1",
    "@radix-ui/react-checkbox": "0.1.5",
    "@radix-ui/react-icons": "1.1.0",
    "fast-json-stable-stringify": "2.1.0",
    "github-slugger": "1.4.0",
    "ioredis": "4.28.5",
    "ms": "3.0.0-canary.1",
    "next": "12.1.1-canary.6",
    "plaiceholder": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "title": "3.4.4",
    "uuid": "8.3.2",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "@types/ioredis": "4.28.8",
    "@types/ms": "0.7.31",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/title": "3.4.1",
    "@types/uuid": "8.3.4",
    "@types/valid-url": "1.0.3"
  },
  "type": "module",
  "sideEffects": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**"
  ]
}
