    "fast-json-stable-stringify": "^2.1.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "plaiceholder": "^2.5.0 || ^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.13.2"
  },
  "devDependencies": {
