{
  "name": "jeromefitzgerald.com",
  "version": "0.0.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/websites.git",
    "directory": "sites/jeromefitzgerald.com"
  },
  "scripts": {
    "---": "@next",
    "analyze": "ANALYZE=true yarn build",
    "build": "CI=1 next build",
    "dev": "next dev",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "start": "next start",
    "start:isr": "next start -H localhost",
    "unused": "yarn next-unused",
    "--- ": "",
    "clean": "rm -rf .turbo && rm -rf ./.next",
    "clean:install": "yarn clean && rm -rf ./.cache && rm -rf node_modules",
    "copy": "echo mkdir -p ./dist && ./scripts/copy.sh",
    "---  ": "@lint",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:prettier": "prettier \"./src/**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\" --ignore-unknown --loglevel warn",
    "lint:typescript": "tsc --noEmit --declaration",
    "lint": "yarn lint:prettier --check && yarn lint:eslint && yarn lint:typescript",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "---   ": "@release",
    "semantic-release": "semantic-release",
    "semantic-release:mono": "semantic-release -e semantic-release-monorepo",
    "---    ": "",
    "---     ": "",
    "test": "echo # jest",
    "---      ": ""
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@jeromefitz/design-system": "3.3.1",
    "@jeromefitz/eslint-config": "1.0.4",
    "@jeromefitz/lint-staged": "1.0.3",
    "@jeromefitz/notion": "3.2.1",
    "@jeromefitz/shared": "*",
    "@jeromefitz/spotify": "2.2.0",
    "@jeromefitz/utils": "2.1.0",
    "@mantine/hooks": "4.2.8",
    "@octokit/core": "3.6.0",
    "@plaiceholder/next": "2.3.0",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-icons": "1.1.1",
    "@stitches/react": "1.2.8",
    "@types/react": "18.0.12",
    "@vercel/fetch": "6.2.0",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.4",
    "emoji-api": "2.0.1",
    "fast-json-stable-stringify": "2.1.0",
    "fathom-client": "3.4.1",
    "framer-motion": "6.3.10",
    "github-slugger": "1.4.0",
    "immer": "9.0.14",
    "ioredis": "5.0.6",
    "kbar": "0.1.0-beta.35",
    "ms": "3.0.0-canary.1",
    "next": "12.1.7-canary.29",
    "next-compose-plugins": "2.2.1",
    "next-seo": "5.4.0",
    "next-sitemap": "3.0.5",
    "next-themes": "0.2.0",
    "next-transpile-modules": "9.0.0",
    "node-emoji": "1.11.0",
    "nprogress": "0.2.0",
    "plaiceholder": "2.3.0",
    "pluralize": "8.0.0",
    "podcast": "2.0.0",
    "ramda": "0.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-swipeable": "7.0.0",
    "react-use": "17.4.0",
    "sharp": "0.30.6",
    "swr": "1.3.0",
    "title": "3.4.4",
    "ts-node": "10.8.1",
    "use-sound": "4.0.1",
    "uuid": "8.3.2",
    "zustand": "3.7.2"
  },
  "devDependencies": {
    "@jeromefitz/eslint-config": "1.0.4",
    "@jeromefitz/prettier-config": "1.0.0",
    "@jeromefitz/tsconfig": "1.0.0",
    "@next/bundle-analyzer": "12.1.6",
    "@types/github-slugger": "1.3.0",
    "@types/ms": "0.7.31",
    "@types/nprogress": "0.2.0",
    "@types/pluralize": "0.0.29",
    "@types/ramda": "0.28.14",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/title": "3.4.1",
    "@types/uuid": "8.3.4",
    "eslint-config-next": "12.1.6",
    "next-unused": "0.0.6"
  },
  "next-unused": {
    "alias": {
      "~components": "src/components/",
      "~config": "src/config/",
      "~context": "src/context/",
      "~custom": "src/custom/",
      "~data": "src/data/",
      "~hooks": "src/hooks/",
      "~lib": "src/lib/",
      "~store": "src/store/",
      "~styles": "src/styles/",
      "~utils": "src/utils/"
    },
    "debug": false,
    "entrypoints": [
      "src/pages"
    ],
    "exclude": [
      "src/data/*"
    ],
    "include": [
      "src/components",
      "src/config",
      "src/context",
      "src/custom",
      "src/data",
      "src/hooks",
      "src/pages",
      "src/routes",
      "src/store",
      "src/styles",
      "src/utils"
    ]
  }
}
