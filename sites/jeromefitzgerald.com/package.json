{
  "name": "jeromefitzgerald.com",
  "version": "0.0.0",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/websites.git",
    "directory": "sites/jeromefitzgerald.com"
  },
  "scripts": {
    "---": "@next",
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next dev",
    "postbuild": "next-sitemap --config next.sitemap.js",
    "start": "next start",
    "unused": "yarn next-unused",
    "--- ": "",
    "clean": "rm -rf .turbo && rm -rf ./.next",
    "clean:install": "yarn clean && rm -rf ./.cache && rm -rf node_modules",
    "copy": "echo mkdir -p ./dist && ./scripts/copy.sh",
    "---  ": "@lint",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:prettier": "prettier \"./src/**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\" --ignore-unknown --loglevel warn",
    "lint:typescript": "tsc --noEmit --declaration",
    "lint": "yarn lint:prettier --check && yarn lint:eslint && yarn lint:typescript",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "---   ": "@release",
    "semantic-release": "semantic-release",
    "semantic-release:mono": "semantic-release -e semantic-release-monorepo",
    "---    ": "",
    "---     ": "",
    "test": "echo # jest",
    "---      ": ""
  },
  "dependencies": {
    "@artsy/fresnel": "3.2.1",
    "@heroicons/react": "1.0.5",
    "@jeromefitz/design-system": "2.0.0",
    "@jeromefitz/notion": "3.0.0",
    "@jeromefitz/spotify": "2.1.5",
    "@jeromefitz/utils": "2.0.0",
    "@notionhq/client": "0.4.13",
    "@octokit/core": "3.5.1",
    "@plaiceholder/next": "2.2.0",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-accessible-icon": "0.1.3",
    "@radix-ui/react-accordion": "0.1.5",
    "@radix-ui/react-alert-dialog": "0.1.5",
    "@radix-ui/react-announce": "0.1.3",
    "@radix-ui/react-aspect-ratio": "0.1.3",
    "@radix-ui/react-avatar": "0.1.3",
    "@radix-ui/react-checkbox": "0.1.4",
    "@radix-ui/react-compose-refs": "0.1.0",
    "@radix-ui/react-dialog": "0.1.5",
    "@radix-ui/react-dropdown-menu": "0.1.4",
    "@radix-ui/react-icons": "1.0.3",
    "@radix-ui/react-popover": "0.1.4",
    "@radix-ui/react-radio-group": "0.1.4",
    "@radix-ui/react-scroll-area": "0.1.3",
    "@radix-ui/react-separator": "0.1.3",
    "@radix-ui/react-switch": "0.1.4",
    "@radix-ui/react-tabs": "0.1.4",
    "@radix-ui/react-tooltip": "0.1.6",
    "@radix-ui/react-utils": "0.0.5",
    "@radix-ui/react-visually-hidden": "0.1.3",
    "@stitches/react": "1.2.6",
    "@vercel/fetch": "6.1.1",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.2.2",
    "emoji-api": "2.0.1",
    "fast-json-stable-stringify": "2.1.0",
    "fathom-client": "3.4.0",
    "github-slugger": "1.4.0",
    "ioredis": "4.28.4",
    "ms": "3.0.0-canary.1",
    "next": "12.0.10",
    "next-seo": "5.1.0",
    "next-themes": "0.0.15",
    "node-emoji": "1.11.0",
    "nprogress": "0.2.0",
    "plaiceholder": "2.2.0",
    "pluralize": "8.0.0",
    "podcast": "2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.2.0",
    "react-use": "17.3.2",
    "sharp": "0.30.0",
    "smoothscroll-polyfill": "0.4.4",
    "swr": "1.2.1",
    "title": "3.4.3",
    "use-sound": "4.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.10",
    "@types/github-slugger": "1.3.0",
    "@types/ioredis": "4.28.7",
    "@types/ms": "0.7.31",
    "@types/nprogress": "0.2.0",
    "@types/pluralize": "0.0.29",
    "@types/title": "3.4.1",
    "@types/uuid": "8.3.4",
    "eslint-config-next": "12.0.10",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "9.0.0",
    "next-sitemap": "2.1.8",
    "next-unused": "0.0.6"
  },
  "next-unused": {
    "alias": {
      "~components": "src/components/",
      "~config": "src/config/",
      "~context": "src/context/",
      "~custom": "src/custom/",
      "~data": "src/data/",
      "~hooks": "src/hooks/",
      "~lib": "src/lib/",
      "~styles": "src/styles/",
      "~utils": "src/utils/"
    },
    "debug": false,
    "entrypoints": [
      "src/pages"
    ],
    "exclude": [
      "src/data/*",
      "src/hooks/useProgress.ts",
      "src/lib/notion/*",
      "src/styles/fonts/*",
      "src/utils/isActiveLink.ts",
      "src/utils/isElementOfType.ts"
    ],
    "include": [
      "src/components",
      "src/config",
      "src/context",
      "src/custom",
      "src/data",
      "src/hooks",
      "src/lib",
      "src/pages",
      "src/styles",
      "src/utils"
    ]
  }
}
