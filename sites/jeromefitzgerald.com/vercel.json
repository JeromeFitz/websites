{
  "version": 2,
  "regions": ["iad"],
  "public": false,
  "cleanUrls": true,
  "build": {
    "env": {
      "NPM_RC": "registry=https://registry.npmjs.org/jeromefitz\n//registry.npmjs.org/:_authToken=${NPM_TOKEN}\n//registry.npmjs.org/jeromefitz/:_authToken=${NPM_TOKEN}\nalways-auth=true\n",
      "NPM_TOKEN": "@npm--jeromefitz--read-token"
    }
  },
  "github": { "enabled": false },
  "trailingSlash": false,
  "headers": [
    {
      "source": "/static/fonts/inter/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/scripts/fathom.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    }
  ]
}
